<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SitterProfile">

    <ImageView
        android:id="@+id/imgGallery"
        android:layout_width="186dp"
        android:layout_height="138dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnCamera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#134F5C"
        android:text="Open Gallery"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgGallery"
        app:layout_constraintVertical_bias="0.089" />


    <TextView
        android:id="@+id/textView_show_full_name"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_profile"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCamera"
        app:layout_constraintVertical_bias="0.103"
        tools:ignore="NotSibling" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textView_show_full_name"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/ic_profile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView_show_full_name"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCamera"
        app:layout_constraintVertical_bias="0.103"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/textView_show_mail"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_profile"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCamera"
        app:layout_constraintVertical_bias="0.232" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/textView_show_mail"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ic_mail"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView_show_mail"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_profile"
        app:layout_constraintVertical_bias="0.08"
        tools:ignore="MissingConstraints" />



    <ImageView
        android:id="@+id/ic_gender"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/gender"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_mail"
        app:layout_constraintVertical_bias="0.102"
        tools:ignore="MissingConstraints,NotSibling" />

    <TextView
        android:id="@+id/textView_show_gender"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_gender"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ic_gender"
        app:layout_constraintTop_toBottomOf="@+id/textView_show_mail"
        app:layout_constraintVertical_bias="0.116"
        tools:ignore="NotSibling" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textView_show_gender"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ic_mobile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/call"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_gender"
        app:layout_constraintVertical_bias="0.12"
        tools:ignore="MissingConstraints,NotSibling" />

    <TextView
        android:id="@+id/textView_show_mobile"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_mobile"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ic_mobile"
        app:layout_constraintTop_toBottomOf="@+id/textView_show_gender"
        app:layout_constraintVertical_bias="0.135"
        tools:ignore="NotSibling" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textView_show_mobile"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textViewWelcome"
        android:layout_width="208dp"
        android:layout_height="30dp"
        android:hint="Loading"
        app:layout_constraintBottom_toTopOf="@+id/imgGallery"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintVertical_bias="0.9" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="230dp" />
</androidx.constraintlayout.widget.ConstraintLayout>