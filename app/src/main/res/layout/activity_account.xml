<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AccountActivity">

    <TextView
        android:id="@+id/textView18"
        android:layout_width="189dp"
        android:layout_height="30dp"
        android:text="My pets profiles"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_show_mobile"
        app:layout_constraintVertical_bias="0.204" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="219dp"
        android:layout_height="50dp"
        android:text="My account"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="152dp"
        android:layout_height="30dp"
        android:layout_marginTop="48dp"
        android:text="Personal info"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.0" />


    <ImageView
        android:id="@+id/imgGallery"
        android:layout_width="122dp"
        android:layout_height="96dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnCamera"
        android:layout_width="252dp"
        android:layout_height="41dp"
        android:backgroundTint="#134F5C"
        android:text="Upload a profile pic"
        app:layout_constraintBottom_toBottomOf="@+id/imgGallery"
        app:layout_constraintEnd_toStartOf="@+id/imgGallery"
        app:layout_constraintHorizontal_bias="0.153"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView_show_full_name"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_profile"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCamera"
        app:layout_constraintVertical_bias="0.131"
        tools:ignore="NotSibling" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textView_show_full_name"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ic_profile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView_show_full_name"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCamera"
        app:layout_constraintVertical_bias="0.131"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView_show_mail"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_profile"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCamera"
        app:layout_constraintVertical_bias="0.218" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/textView_show_mail"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/ic_mail"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView_show_mail"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_profile"
        app:layout_constraintVertical_bias="0.041"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ic_gender"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/gender"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_mail"
        app:layout_constraintVertical_bias="0.032"
        tools:ignore="MissingConstraints,NotSibling" />

    <TextView
        android:id="@+id/textView_show_gender"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_gender"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toEndOf="@+id/ic_gender"
        app:layout_constraintTop_toBottomOf="@+id/textView_show_mail"
        app:layout_constraintVertical_bias="0.034"
        tools:ignore="NotSibling" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textView_show_gender"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ic_mobile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/call"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_gender"
        app:layout_constraintVertical_bias="0.045"
        tools:ignore="MissingConstraints,NotSibling" />

    <TextView
        android:id="@+id/textView_show_mobile"
        android:layout_width="323dp"
        android:layout_height="23dp"
        android:layout_alignStart="@id/ic_mobile"
        android:hint="Loading"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toEndOf="@+id/ic_mobile"
        app:layout_constraintTop_toBottomOf="@+id/textView_show_gender"
        app:layout_constraintVertical_bias="0.051"
        tools:ignore="NotSibling" />

    <Button
        android:id="@+id/button4"
        android:layout_width="142dp"
        android:layout_height="42dp"
        android:layout_marginEnd="12dp"
        android:backgroundTint="#134F5C"
        android:text="View details"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.237"

        tools:ignore="ViewBindingType" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="70dp"
        android:layout_height="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName9"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.231"
        tools:srcCompat="@tools:sample/avatars" />

    <EditText
        android:id="@+id/editTextTextPersonName9"
        android:layout_width="155dp"
        android:layout_height="43dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.232" />

</androidx.constraintlayout.widget.ConstraintLayout>